{
  "name": "@motionpicture/social-login",
  "version": "1.0.0-alpha.1",
  "description": "Social Plus API client library for Node.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "doc/",
    "example/",
    "lib/"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "author": "Motionpicture co.,ltd.",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint && npm audit",
    "clean": "rimraf \"src/**/*.js ./lib test/**/*.js npm-debug.log* ./doc ./coverage ./.nyc_output\"",
    "doc": "jsdoc -c jsdoc.json",
    "mocha": "mocha",
    "nyc": "nyc mocha \"src/**/*.spec.ts\"",
    "coverage": "npm run nyc | coveralls",
    "snyk": "snyk wizard",
    "test": "npm run check",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/http-status": "^0.2.30",
    "@types/mocha": "^2.2.46",
    "@types/nock": "^8.2.1",
    "@types/power-assert": "^1.4.29",
    "@types/sinon": "^2.3.7",
    "coveralls": "^3.0.2",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nock": "^9.1.6",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.5",
    "snyk": "^1.68.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.2",
    "typescript": "^2.6.2"
  },
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=6.4.0"
  },
  "dependencies": {
    "@types/request": "^2.47.0",
    "debug": "^3.1.0",
    "http-status": "^1.0.1",
    "request": "^2.87.0"
  }
}
